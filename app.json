{
  "expo": {
    "name": "Appcultor",
    "slug": "Appcultor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android":{
      "adaptiveIcon": { 
        "foregroundImage": "./assets/Apicultor.png",
        "backgroundImage": "./assets/fundoApicultor.png" 
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
